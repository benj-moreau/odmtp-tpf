  a owl:Class ;
  rdfs:label "Hashtag"@el, "Hashtag"@en .

<http://www.influenceTracker.com/ontology#QualityMetrics>
  a owl:Class ;
  rdfs:label "Ποιοτικές Μετρήσεις"@el, "Quality Metrics"@en .

<http://www.influenceTracker.com/ontology#TwitterAccount>
  a owl:Class ;
  rdfs:label "Λογαριασμός Twitter"@el, "Twitter Account"@en ;
  rdfs:subClassOf foaf:OnlineAccount .

<http://www.influenceTracker.com/ontology#URL>
  a owl:Class ;
  rdfs:label "URL"@el, "URL"@en .

<http://www.influenceTracker.com/ontology#User>
  a owl:Class ;
  rdfs:label "Χρήστης"@el, "User"@en ;
  rdfs:subClassOf foaf:Agent .

foaf:Agent
  a owl:Class ;
  rdfs:label "Πράκτορας"@el, "Agent"@en .

foaf:Document
  a owl:Class ;
  rdfs:label "Έγγραφο"@el, "Document"@en .

foaf:Image
  a owl:Class ;
  rdfs:label "Φωτογραφία"@el, "Image"@en ;
  rdfs:subClassOf foaf:Document .

foaf:OnlineAccount
  a owl:Class ;
  rdfs:label "Online Λογαριασμός"@el, "Online Account"@en .
