schema:sdPublisher a rdf:Property ;
    rdfs:label "sdPublisher" ;
    dct:source <https://github.com/schemaorg/schemaorg/issues/1886> ;
    schema:category "issue-1886" ;
    schema:domainIncludes schema:CreativeWork ;
